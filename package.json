{
  "name": "transactions-scanner",
  "private": true,
  "version": "0.0.1",
  "displayName": "Transaction Scanner",
  "author": "Nikolai Maidan",
  "description": "An extension for your browser that allows you to track the transaction status across various cryptocurrency networks.",
  "scripts": {
    "dev": "vite",
    "build_v3": "MANIFEST_VERSION=3 vite build --mode production",
    "build_v2": "MANIFEST_VERSION=2 vite build --mode production",
    "watch_v3": "MANIFEST_VERSION=3 vite build --w --mode development",
    "watch_v2": "MANIFEST_VERSION=2 vite build --w --mode development",
    "preview": "vite preview"
  },
  "dependencies": {
    "@samrum/vite-plugin-web-extension": "^5.0.0",
    "@vueuse/core": "^10.5.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.28.1",
    "eslint-restricted-globals": "^0.2.0",
    "pinia": "^2.1.6",
    "prettier": "^3.0.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.48.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "eslint-plugin-vue": "^9.17.0",
    "postcss": "^8.4.31",
    "sass": "^1.66.1",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}
